.span2.show-grid.columns{:style=>'width:115px;'}
  .tabbable.tabs-left
    %ul.nav.nav-tabs
      - # Admin menu items can be found in /app/helpers/admin_helper.rb
      - admin_tabs.each do |action|
        %li{:class=>('active' if params[:controller] == "admin/#{action}")}
          %a{:href=>"/admin/#{action}"}
            = t("admin.tabs.#{action}")

